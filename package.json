{
  "name": "color-requests",
  "version": "1.0.0",
  "description": "Allows for users to request a color for their name",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src -d dist",
    "dev": "export APP_ROOT=$PWD && nodemon --exec babel-node ./src/index.js",
    "start": "export APP_ROOT=$PWD && pnpm run build&& cd dist&& node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheMrGong/ColorRequsts.git"
  },
  "keywords": [
    "discord",
    "bot",
    "color-request",
    "javascript",
    "node",
    "npm"
  ],
  "author": "TheMrGong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheMrGong/ColorRequsts/issues"
  },
  "homepage": "https://github.com/TheMrGong/ColorRequsts#readme",
  "dependencies": {
    "canvas": "^2.8.0",
    "discord.js": "^12.5.3",
    "gif-frames": "^1.0.1",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "rgb-hsv": "^1.0.0",
    "sharp": "^0.28.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "nodemon": "^1.19.4",
    "rimraf": "^3.0.2"
  }
}
